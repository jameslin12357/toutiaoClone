    </ul>
    <form class="form-inline my-2 my-lg-0">
      <input class="form-control mr-sm-2" type="search" placeholder="请输入要搜索的内容" aria-label="搜索">
      <button class="btn btn-outline-success my-2 my-sm-0" type="submit">搜索</button>
    </form>
  </div>
  </div>
</nav>

<div class="container">
<div class="row">
  <div class="col-md-3">
    <div class="list-group mt-10">
      <a href="#" class="list-group-item list-group-item-action active">
        推荐
      </a>
      <a href="#" class="list-group-item list-group-item-action">热点</a>
      <a href="#" class="list-group-item list-group-item-action">直播</a>
      <a href="#" class="list-group-item list-group-item-action">科技</a>
      <a href="#" class="list-group-item list-group-item-action">娱乐</a>
      <a href="#" class="list-group-item list-group-item-action">游戏</a>
      <a href="#" class="list-group-item list-group-item-action">体育</a>
      <a href="#" class="list-group-item list-group-item-action">汽车</a>
      <a href="#" class="list-group-item list-group-item-action">财经</a>
      <a href="#" class="list-group-item list-group-item-action">搞笑</a>
    </div>
  </div>
  <div class="col-md-9">
    <div class="mt-10">
      <% for(var i=0; i<results.length; i++) {%>
        <div class="card">

          <div class="card-body">
            <a href="/videos/<%= results[i].videoid %>"><h5 class="card-title"><%= results[i].title %></h5></a>
            <div>
              <a href="/users/<%= results[i].id %>"><%= results[i].username %></a>
                <span>moment(<%= results[i].created %>).format("MMM Do YY")</span>
            </div>
          </div>
        </div>
      <% } %>
    </div>

  </div>
</div>


</div>

